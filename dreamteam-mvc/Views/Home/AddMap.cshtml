@{
    ViewData["Title"] = "Ajout map";

    var Name = "";
    var Place = "";
    var MapUrl = "";
}
<h1 style="color: white;">@ViewData["Title"]</h1>

<form asp-controller="Home" asp-action="AjoutMap" method="post">
    <div>
        <h3 style="color: white;">Nom de la map :</h3>
        <input asp-for="@Name" />
        <h3 style="color: white;">Pays :</h3>
        <input asp-for="@Place" />
        <h3 style="color: white;">Url :</h3>
        <input asp-for="@MapUrl" />
    </div>
    <button type="submit">Ajouter</button>
</form>
